{
  "name": "example-project",
  "version": "0.1.0",
  "description": "A Coyotiv School Of Software Design example Project",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest --testEnvironment=node --verbose --forceExit --watchAll --maxWorkers=1",
    "test-coverage": " jest --collect-coverage --testEnvironment=node --forceExit --maxWorkers=1",
    "lint": "lint-staged",
    "lint-only": "eslint .",
    "lint-all": "pretty-quick && eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "pretty-quick --staged"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3vincent/example-project.git"
  },
  "keywords": [],
  "author": "Ben Sukstorf <bsukstorf@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3vincent/example-project/issues"
  },
  "homepage": "https://github.com/3vincent/example-project#readme",
  "devDependencies": {
    "connect-livereload": "^0.6.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "livereload": "^0.9.3",
    "mongoose": "^5.12.13",
    "mongoose-autopopulate": "^0.14.0",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "socket.io": "^4.1.2"
  }
}
